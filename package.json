{
  "name": "crackboard",
  "displayName": "crackboard",
  "description": "tpot leaderboard productivity tracker",
  "publisher": "crackboard",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boganworld/crackboard-vsc.git"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
        {
            "command": "crackboard.inputSessionKey",
            "title": "Crackboard: Set Session Key"
        }
        ],
        "configuration": {
            "type": "object",
            "title": "Crackboard Configuration",
            "properties": {
                "crackboard.sessionKey": {
                    "type": "string",
                    "default": "",
                    "description": "Session key for Crackboard."
                }
            }
        }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
